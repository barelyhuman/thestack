{
  "name": "tillwhen-backend",
  "version": "1.0.0",
  "main": "index.js",
  "packageManager": "pnpm@8.3.1",
  "scripts": {
    "dev": "concurrently 'nr dev:server' 'nr dev:css'",
    "dev:server": "npx nodemon -e ts,njk,css -r tsconfig-paths/register src/server.ts",
    "dev:worker": "npx tsnd --exit-child -r tsconfig-paths/register src/worker.ts",
    "dev:css": "npx tailwindcss -i ./src/styles/tailwind.css -o ./src/public/styles.min.css --watch",
    "start": "npx ts-node --transpileOnly -r tsconfig-paths/register src/server.ts",
    "css": "npx tailwindcss -i ./src/styles/tailwind.css -o ./src/public/styles.min.css --minify",
    "test:types": "tsc",
    "fix": "npx prettier --write .",
    "test": "cross-env NODE_ENV=TEST vitest ./tests",
    "test:ci": "cross-env NODE_ENV=TEST vitest run ./tests",
    "db:migrate": "npx prisma migrate deploy",
    "db:reset": "npx prisma migrate reset",
    "db:push": "npx prisma db push",
    "db:make:migration": "npx prisma migrate dev",
    "prepare": "husky install"
  },
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "4.15.0",
    "body-parser": "^1.20.2",
    "bull": "^4.10.4",
    "compression": "^1.7.4",
    "connect-redis": "^7.1.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "csrf": "^3.1.0",
    "date-fns": "^2.30.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-flash": "^0.0.2",
    "express-jsdoc-swagger": "^1.8.0",
    "express-limiter": "^1.6.1",
    "express-session": "^1.17.3",
    "helmet": "^7.0.0",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^9.0.0",
    "morgan": "^1.10.0",
    "ms": "^2.1.3",
    "nodemailer": "^6.9.3",
    "nunjucks": "^3.2.4",
    "serve-static": "^1.15.0",
    "yup": "^1.2.0"
  },
  "devDependencies": {
    "@antfu/ni": "^0.21.4",
    "@barelyhuman/prettier-config": "^1.1.0",
    "@tailwindcss/forms": "^0.5.3",
    "@types/connect-redis": "^0.0.20",
    "@types/express": "^4.17.17",
    "@types/express-flash": "^0.0.2",
    "@types/express-session": "^1.17.7",
    "@types/node": "^20.3.1",
    "axios": "^1.4.0",
    "concurrently": "^8.2.0",
    "cross-env": "^7.0.3",
    "husky": ">=7",
    "ioredis-mock": "^8.7.0",
    "lint-staged": ">=10",
    "nodemon": "^2.0.22",
    "prettier": "^2.8.8",
    "prettier-plugin-twig-nunjucks-melody": "^0.1.0",
    "pretty-quick": "^3.1.3",
    "prisma": "^4.15.0",
    "tailwindcss": "^3.3.2",
    "test-listen": "^1.1.0",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.6",
    "vite": "^4.3.9",
    "vitest": "^0.32.2"
  },
  "lint-staged": {
    "*.{ts,tsx,js,css,md}": "prettier --write"
  }
}
