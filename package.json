{
  "name": "decorator-mvc",
  "version": "1.0.0",
  "main": "index.js",
  "packageManager": "pnpm@8.3.1",
  "scripts": {
    "dev": "npx tsnd --exit-child -r tsconfig-paths/register src/server.ts",
    "start": "npx ts-node --transpileOnly -r tsconfig-paths/register src/server.ts",
    "test:types": "tsc",
    "fix": "npx pretty-quick",
    "test": "vitest ./tests",
    "db:migrate": "npx prisma migrate deploy",
    "db:reset": "npx prisma migrate reset",
    "db:push": "npx prisma db push",
    "db:make:migration": "npx prisma migrate dev"
  },
  "prettier": "@barelyhuman/prettier-config",
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "4.15.0",
    "body-parser": "^1.20.2",
    "bull": "^4.10.4",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "defu": "^6.1.2",
    "express": "^4.18.2",
    "express-limiter": "^1.6.1",
    "helmet": "^7.0.0",
    "ioredis": "^5.3.2",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "@barelyhuman/prettier-config": "^1.1.0",
    "@types/express": "^4.17.17",
    "@types/node": "^20.3.1",
    "axios": "^1.4.0",
    "nodemon": "^2.0.22",
    "prettier": "^2.8.8",
    "pretty-quick": "^3.1.3",
    "prisma": "^4.15.0",
    "test-listen": "^1.1.0",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "vite": "^4.3.9",
    "vitest": "^0.32.2"
  }
}
